@model List<Category> //A model of list of category passed from the controller

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2>Category</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="" asp-action="" class="btn btn-primary"><i class="bi bi-plus-circle"></i> Add Category</a>
        </div>
    </div>
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Category Name</th>
                <th>Display Order</th>
            </tr>
        </thead>
        <tbody>
            <!--Use to use C# in html-->
            @foreach (var obj in Model)
            {
                <tr>
                    <td>@obj.Name</td>
                    <td>@obj.DisplayOrder</td>
                </tr>
            }
        </tbody>

    </table>
</div>
